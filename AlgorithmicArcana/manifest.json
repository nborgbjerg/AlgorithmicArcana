{


  "name": "Algorithmic Arcana",
    "version": "0.1",
    "description": "Discover what the algorithm is trying to tell you!",
    "manifest_version": 2,

    "web_accessible_resources":[
        "images/arcana02.png"
      ],

    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["sketch.js"]

        }
      ],
      "icons": {
        "16": "/icons/card16.png",
        "32": "/icons/card32.png",
        "48": "/icons/card48.png",
        "128": "/icons/card128.png"
      }

  }
